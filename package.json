{
  "name": "@navikt/dp-auth",
  "version": "0.3.13",
  "description": "Autentiseringsmodul for node apper som bruker IncomingMessage som request-objekt til dagpengeteamet",
  "scripts": {
    "build-lib": "tsc --noEmit false",
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "prepublishOnly": "npm test",
    "prepack": "npm run build-lib",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install",
    "clean": "rm -rf ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/dp-auth.git"
  },
  "author": "NAV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navikt/dp-auth/issues"
  },
  "homepage": "https://github.com/navikt/dp-auth#readme",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "next": ">=12"
  },
  "optionalDependencies": {
    "prom-client": ">=15"
  },
  "dependencies": {
    "jose": "^4.9.1",
    "lodash": "^4.17.21",
    "node-cache": "^5.1.2",
    "openid-client": "^5.1.9",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@jest/types": "^29.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.7.0",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.4",
    "node-mocks-http": "^1.14.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*/": [
        "dist/*"
      ],
      "obo-providers": [
        "dist/obo-providers/index.d.ts"
      ],
      "obo-providers/withPrometheus": [
        "dist/obo-providers/withPrometheus.d.ts"
      ],
      "identity-providers": [
        "dist/identity-providers/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./obo-providers": "./dist/obo-providers/index.js",
    "./obo-providers/withPrometheus": "./dist/obo-providers/withPrometheus.js",
    "./identity-providers": "./dist/identity-providers/index.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ]
}
