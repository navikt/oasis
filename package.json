{
  "name": "@navikt/dp-auth",
  "version": "0.1.0",
  "description": "Autentiseringsmodul for NextJS appene til dagpengeteamet",
  "scripts": {
    "build-lib": "tsc --noEmit false",
    "test": "jest",
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "prepublishOnly": "npm test",
    "prepack": "npm run build-lib",
    "type-check": "tsc --pretty --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/navikt/dp-auth.git"
  },
  "author": "NAV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navikt/dp-auth/issues"
  },
  "homepage": "https://github.com/navikt/dp-auth#readme",
  "peerDependencies": {
    "next": "^12.0.0"
  },
  "dependencies": {
    "env-var": "^7.1.1",
    "jose": "^4.8.3",
    "lodash": "^4.17.21",
    "cookie": "^0.4.1",
    "openid-client": "^5.1.8",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@jest/types": "^28.1.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.43.0",
    "node-mocks-http": "^1.11.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "main": "./dist/api/index.js",
  "exports": {
    ".": "./dist/api/index.js",
    "./client": "./dist/client/index.js",
    "./server": "./dist/server/index.js"
  },
  "typesVersions": {
    "*": {
      "api": [
        "dist/api/index.d.ts"
      ],
      "client": [
        "dist/client/index.d.ts"
      ],
      "server": [
        "dist/server/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
